{
  "name": "nuxt-ts-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nuxt starter project",
  "main": "index.js",
  "scripts": {
    "dev": "nuxt-ts dev -c nuxt.config.ts",
    "build": "nuxt-ts build -c nuxt.config.ts && tsc nuxt.config.ts",
    "start": "nuxt start -c nuxt.config.js",
    "generate": "taxios-generate https://petstore.swagger.io/v2/swagger.json -o ./types/generated/PetStore.ts -e PetStore",
    "lint": "prettier --check . && eslint --ext .ts,.js,.vue .",
    "lint:fix": "prettier --write . && eslint --fix --ext .ts,.js,.vue ."
  },
  "license": "ISC",
  "dependencies": {
    "@nuxtjs/axios": "^5.12.0",
    "@simplesmiler/taxios": "^0.1.4",
    "nuxt": "^2.14.0",
    "nuxt-composition-api": "^0.11.0",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.0",
    "@nuxt/typescript-build": "^2.0.2",
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/eslint-config-typescript": "^2.1.0",
    "@simplesmiler/taxios-generate": "^0.1.4",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  }
}
